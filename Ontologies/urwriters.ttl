###  https://purl.archive.org/urwriters#Migrating
urw:Migrating rdf:type owl:Class ;
              rdfs:subClassOf [ owl:intersectionOf ( urw:Activity
                                                     [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                              owl:someValuesFrom urw:Agent
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                              owl:allValuesFrom urw:Agent
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
              rdfs:comment "Migrating is a sub-class of an Event, which is used within the modeling of the Migration biographical pattern. It is in the setting of a Migration (urw:hasSetting) and it has an Agent as participant (urw:hasParticipant)"@en .


###  https://purl.archive.org/urwriters#Migration
urw:Migration rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( urw:Situation
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty urw:isSettingFor ;
                                                           owl:someValuesFrom urw:Migrating
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty urw:isSettingFor ;
                                                           owl:someValuesFrom urw:MigrationReason
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty urw:isSettingFor ;
                                                           owl:someValuesFrom urw:Place
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty urw:satisfies ;
                                                           owl:someValuesFrom urw:MigrationPlan
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf urw:Situation ;
              rdfs:comment "The situation in which a person migrates. Such situation is modeled from the Basic Plan Execution ODP"@en .


###  https://purl.archive.org/urwriters#MigrationPlan
urw:MigrationPlan rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( urw:Description
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty urw:hasComponent ;
                                                               owl:someValuesFrom urw:MigrationReason
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty urw:isSatisfiedBy ;
                                                               owl:someValuesFrom urw:Migration
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf urw:Description ;
                  rdfs:comment """The plan of migrationg, satisfied by the Migration itself. 
This class iswas derived from the BasicPlanExecution ODP."""@en .


###  https://purl.archive.org/urwriters#MigrationReason
urw:MigrationReason rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( urw:Description
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty urw:defines ;
                                                                 owl:someValuesFrom urw:MigrationRole
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf urw:Description ;
                    rdfs:comment """The reason of a person to migrate. It defines their role (eg: \"economic reson\" results in the role \"economic migrant\"). 
This class iswas derived from the BasicPlanExecution ODP."""@en .


###  https://purl.archive.org/urwriters#MigrationRole
urw:MigrationRole rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( urw:Role
                                                             [ rdf:type owl:Restriction ;
